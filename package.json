{
  "name": "bloki",
  "version": "1.0.0",
  "description": "A customizable react flexbox grid component",
  "main": "lib/index.js",
  "scripts": {
    "build-dev": "webpack --config webpack.config.js --progress --profile --colors",
    "start": "webpack-dev-server --mode development --progress --profile --colors",
    "dist": "babel src --out-dir lib",
    "test": "jest",
    "lint": "eslint src"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/tests/index.js"
  },
  "author": "Ren Rizzolo",
  "keywords": [
    "react",
    "grid",
    "flexbox"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renrizzolo/bloki.git"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.1",
    "emotion": "9.2.3",
    "react-emotion": "9.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-adapter-future": "^1.1.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
